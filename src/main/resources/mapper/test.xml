<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="biao.community.dao.DTest">
    <update id="selectB" parameterType="int" >
        update g_sell set g_view = #{sum} where g_id = #{id}
    </update>

    <update id="setToBeRead" parameterType="int">
        update user set bToBeRead = #{b} , gToBeRead = #{g} where u_id =#{u_id}
    </update>

</mapper>
